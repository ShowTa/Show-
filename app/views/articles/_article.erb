<div class='article'>
  <h2>作成者 : </h2>
  <%= article.title %>
  <div class=''>
    <%= article.content %>
  </div>
  <p>
    <%= article.user.name %>
  </p>

  <%= link_to '詳細', article_path(article) %>
  <%= link_to '編集', edit_article_path(article) %>
  <%= link_to '削除', article_path(article), method: :DELETE %>
</div>

<%= link_to '新規投稿', new_article_path %>
